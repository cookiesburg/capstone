<body id="cooker-home">

  <%= render partial: "partials/nav" %>

  <div class="cooker-welcome">
    <h2 align="center"> Welcome back, <%= current_user.username %></h2>
    <h3> You have <%= patrons.count %> patrons this week</h3>
  </div>



  <%= react_component("CookerProfileHome", props: {cooker: @cooker, cook: current_user.username}, prerender: true) %>

  <div class="cook-button">
    <%= link_to "edit", edit_cooker_path(current_user.cooker.id), method: :get, class:"btn btn-info subscribe-button" %>
  </div>

  <%= react_component("PatronList", props: {patrons: patrons}, prerender: true) %>

</body>
