<body id="cooker-home">

  <%= render partial: "partials/nav" %>

  <%= react_component("CookWelcome", props: {patrons: patrons, cook: current_user.username}, prerender: true) %>

  <%= react_component("CookerProfileHome", props: {cooker: @cooker, cook: current_user.username}, prerender: true) %>

  <div class='editme'>
